<%= render 'list' %>

<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @article.title %>
</p>


<p>
  <strong>Text html:</strong>
  <%= @article.text_html.html_safe %>
</p>

<%= link_to 'Add', new_article_path(@article) %> |
<%= link_to 'Edit', edit_article_path(@article) %> |
<%= link_to 'Back', parent_article_path(@article) %>
